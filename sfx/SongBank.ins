    pan    = 64;

    sound = Percussive_absorb_note_0Sound;
    sound = Percussive_absorb_note_1Sound;
    sound = Percussive_absorb_note_2Sound;
    sound = Percussive_absorb_note_3Sound;
    sound = Percussive_attack_landSound;
    sound = Percussive_fadeinSound;
    sound = Percussive_fadeoutSound;
    sound = Percussive_jumpSound;
    sound = Percussive_sword_outSound;
}

bank SongBank 
{
    sampleRate = 32000;
    instrument [0] = Percussion_Kit;

}

