envelope defaultenv
{
    attackTime    = 0;
    attackVolume  = 127;
    decayTime   = 500000;
    decayVolume   = 100;
    releaseTime   = 200000;
    
}


keymap Percussive_absorb_note_0Keymap
{
    velocityMin = 0;
    velocityMax = 127;
    keyMin      = 10;
    keyMax      = 10;
    keyBase     = 10;
    detune      = 0;
}

sound Percussive_absorb_note_0Sound
{
    use ("compressed_samples/000_Percussive_absorb_note_0.aifc");
    
    pan    = 64;
    volume = 127;
    keymap = Percussive_absorb_note_0Keymap;
    envelope = defaultenv;
}

